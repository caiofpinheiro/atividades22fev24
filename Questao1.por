programa {
  funcao inicio() {
    real soma, a, b, c


  escreva("Digite o valor de A: ")
  leia(a)

  escreva("Digite o valor de B: ")
  leia(b)

  escreva("Digite o valor de C: ")
  leia(c)

  soma = (a + b)

  se (soma < c) 
    escreva("A soma de a + b é menor que C")

  senao
    escreva("A soma de a + b é maior que C")
  

  }
}
